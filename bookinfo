package com.example.boimela;

public class bookinfo {

    public String bookName;
    public String author;
    public String stall;
    public int stallNO;
    public double price;

    public bookinfo() {

    }


    public bookinfo(String bookName, String author, String stall, int stallNO, double price) {

        this.bookName = bookName;
        this.author = author;
        this.stall = stall;
        this.stallNO = stallNO;
        this.price = price;
    }




}
